// misc-pres-well-data.component.ts
readonly selectedWellData = computed(() => {
  const selectedId = this.reportStore.selectedWellId(); // From chip
  const response = this.wellStore.response(); // From JSON details

  if (!selectedId || !response?.data) return null;

  // Search through the detailed data array
  return response.data.find(item => {
    // Note: In your JSON, epANum is inside DRLG_OP_STATUS[0]
    const detailedId = item.DRLG_OP_STATUS?.[0]?.epANum;
    
    // Use loose equality (==) or String conversion to handle '98730' vs 98730
    return String(detailedId) === String(selectedId);
  }) ?? null;
});
