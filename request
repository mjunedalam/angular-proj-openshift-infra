package com.aramco.easd.dailyoperations.mappers;


import org.apache.ibatis.annotations.Mapper;
import org.apache.ibatis.annotations.Param;

@Mapper
public interface DrillingEyeMapper {

    String fetchDrillingEyeData(@Param("jsonIn") String jsonIn);
}
package com.aramco.easd.dailyoperations.services;


import com.aramco.easd.dailyoperations.mappers.DrillingEyeMapper;
import com.aramco.easd.dailyoperations.models.DrillingEyeRequestModel;
import com.aramco.easd.dailyoperations.util.JsonUtil;
import jakarta.enterprise.context.ApplicationScoped;
import jakarta.inject.Inject;

@ApplicationScoped
public class DrillingEyeService {

    @Inject
    DrillingEyeMapper drillingEyeMapper;

    public String loadDrillingEyeData(DrillingEyeRequestModel drillingEyeRequestModel) {
        String inputJson = JsonUtil.toJson(drillingEyeRequestModel);
        return drillingEyeMapper.fetchDrillingEyeData(inputJson);
    }


}
package com.aramco.easd.dailyoperations.util;

import com.aramco.easd.dailyoperations.models.DrillingEyeRequestModel;
import com.fasterxml.jackson.databind.ObjectMapper;

public class JsonUtil {

    private static final ObjectMapper mapper = new ObjectMapper();

    public static String toJson(DrillingEyeRequestModel drillingEyeRequestModel) {
        try {
            return mapper.writeValueAsString(drillingEyeRequestModel);
        } catch (Exception e) {
            throw new RuntimeException("Failed to serialize DrLgRequest to JSON", e);
        }
    }

}
