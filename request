package com.aramco.easd.dailyoperations.services;

import com.aramco.easd.dailyoperations.dto.ApiResponse;
import com.aramco.easd.dailyoperations.mappers.DrillingEyeMapper;
import com.aramco.easd.dailyoperations.models.DrillingEyeRequestModel;
import com.aramco.easd.dailyoperations.util.JsonUtil;
import jakarta.enterprise.context.ApplicationScoped;
import jakarta.inject.Inject;
import jakarta.ws.rs.core.Response;


import java.util.HashMap;
import java.util.Map;
import java.util.logging.Level;
import java.util.logging.Logger;

@ApplicationScoped
public class DrillingEyeService {

    private static final Logger LOGGER = Logger.getLogger(DrillingEyeService.class.getName());

    @Inject
    DrillingEyeMapper drillingEyeMapper;

    @Inject
    JsonUtil jsonUtil;

    public ApiResponse<String> loadDrillingEyeData(DrillingEyeRequestModel drillingEyeRequestModel) {
        try {
            if (drillingEyeRequestModel == null) {
                LOGGER.warning("Null request model received.");
                return new ApiResponse.ApiResponseBuilder<String>()
                        .withStatusCode(Response.Status.BAD_REQUEST.getStatusCode())
                        .withError(true)
                        .withMessage("Request model cannot be null.")
                        .withData(null)
                        .build();
            }
            String inputJson = jsonUtil.toJson(drillingEyeRequestModel);
            LOGGER.info("Processing request with input JSON: " + inputJson);

            Map<String, Object> params = new HashMap<>();
            params.put("jsonIn", inputJson);

            drillingEyeMapper.fetchDrillingEyeData(params);

            Object result = params.get("jsonOut");
            LOGGER.info("Result from stored procedure: " + result);

            if (result == null || result.toString().isEmpty()) {
                LOGGER.info("No data found for the given request.");
                return new ApiResponse.ApiResponseBuilder<String>()
                        .withStatusCode(Response.Status.NOT_FOUND.getStatusCode())
                        .withError(true)
                        .withMessage("No data found for the given request.")
                        .withData(null)
                        .build();
            }

            LOGGER.info("Data retrieved successfully.");
            return new ApiResponse.ApiResponseBuilder<String>()
                    .withStatusCode(Response.Status.OK.getStatusCode())
                    .withError(false)
                    .withMessage("Data retrieved successfully.")
                    .withData(result.toString())
                    .build();

        } catch (Exception e) {
            LOGGER.log(Level.INFO, "An error occurred while processing the request", e);
            return new ApiResponse.ApiResponseBuilder<String>()
                    .withStatusCode(Response.Status.INTERNAL_SERVER_ERROR.getStatusCode())
                    .withError(true)
                    .withMessage("An error occurred while processing your request: " + e.getMessage())
                    .withData(null)
                    .build();
        }
    }

    public ApiResponse<String> loadDrillingDataBtEpANum(String epAnum) {
        try {
            if (epAnum == null) {
                LOGGER.warning("Null request model received.");
                return new ApiResponse.ApiResponseBuilder<String>()
                        .withStatusCode(Response.Status.BAD_REQUEST.getStatusCode())
                        .withError(true)
                        .withMessage("Request model cannot be null.")
                        .withData(null)
                        .build();
            }
            LOGGER.info("Processing request with input JSON: " + epAnum);

            Map<String, Object> params = new HashMap<>();
            params.put("jsonIn", epAnum);

            drillingEyeMapper.fetchDrillingEyeData(params);

            Object result = params.get("jsonOut");
            LOGGER.info("Result from stored procedure: " + result);

            if (result == null || result.toString().isEmpty()) {
                LOGGER.info("No data found for the given request.");
                return new ApiResponse.ApiResponseBuilder<String>()
                        .withStatusCode(Response.Status.NOT_FOUND.getStatusCode())
                        .withError(true)
                        .withMessage("No data found for the given request.")
                        .withData(null)
                        .build();
            }

            LOGGER.info("Data retrieved successfully.");
            return new ApiResponse.ApiResponseBuilder<String>()
                    .withStatusCode(Response.Status.OK.getStatusCode())
                    .withError(false)
                    .withMessage("Data retrieved successfully.")
                    .withData(result.toString())
                    .build();

        } catch (Exception e) {
            LOGGER.log(Level.INFO, "An error occurred while processing the request", e);
            return new ApiResponse.ApiResponseBuilder<String>()
                    .withStatusCode(Response.Status.INTERNAL_SERVER_ERROR.getStatusCode())
                    .withError(true)
                    .withMessage("An error occurred while processing your request: " + e.getMessage())
                    .withData(null)
                    .build();
        }
    }
}
