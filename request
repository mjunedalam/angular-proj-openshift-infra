code below FYR
package com.aramco.easd.dailyoperations.resources;


import com.aramco.easd.dailyoperations.dto.ApiResponse;
import com.aramco.easd.dailyoperations.models.DrillingEyeRequestModel;
import com.aramco.easd.dailyoperations.services.DrillingEyeService;
import jakarta.inject.Inject;
import jakarta.ws.rs.Consumes;
import jakarta.ws.rs.POST;
import jakarta.ws.rs.Path;
import jakarta.ws.rs.Produces;
import jakarta.ws.rs.core.MediaType;
import jakarta.ws.rs.core.Response;

import java.util.logging.Level;
import java.util.logging.Logger;

@Path("/drilling-eye")
@Produces(MediaType.APPLICATION_JSON)
public class DrillingEyeResource {

    @Inject
    DrillingEyeService drillingEyeService;

    private static final Logger LOGGER = Logger.getLogger(DrillingEyeResource.class.getName());

    @POST
    @Consumes(MediaType.APPLICATION_JSON)
    public Response getDrillingEyeData(DrillingEyeRequestModel request) {
        LOGGER.log(Level.INFO, "getDrillingEyeData executed with ", request);
        ApiResponse<String> apiResponse = drillingEyeService.loadDrillingEyeData(request);
        return apiResponse.buildResponse();
    }
}

