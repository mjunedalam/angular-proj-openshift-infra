        {call EXAD.EXAD_AGWA_QUERIES_PKG.drlg_mr(
                #{jsonIn,  jdbcType=CLOB, mode=INOUT},
                #{jsonOut, jdbcType=CLOB, mode=OUT}
              )}
    </select>



